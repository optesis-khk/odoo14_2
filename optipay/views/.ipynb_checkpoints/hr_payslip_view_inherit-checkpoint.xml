<odoo>  
   <record id="view_hr_payslip_form_inherit" model="ir.ui.view">
        <field name="name">hr.payslip_form_inherit</field>
        <field name="model">hr.payslip</field>
        <field name="inherit_id" ref="hr_payroll.view_hr_payslip_form"/>
        <field name="arch" type="xml">
            <data> 
              <xpath expr="//page[@name='worked_days_input']" position="replace">
                 <page string="Jours travaillés &amp; entrées">
                                <separator string="Worked Days"/>
                                <field name="worked_days_line_ids">
                                    <tree string="Worked Days" editable="bottom">
                                        <field name="name"/>
                                        <field name="code"/>
                                        <field name="number_of_days" sum="Total Working Days"/>
                                        <field name="number_of_hours"/>
                                        <field name="contract_id"/>
                                        <field name="sequence" invisible="True"/>
                                    </tree>
                                    <form string="Jours travaillés">
                                        <group col="4">
                                            <field name="name"/>
                                            <field name="code"/>
                                            <field name="sequence"/>
                                            <field name="number_of_days"/>
                                            <field name="number_of_hours"/>
                                            <field name="contract_id"/>
                                        </group>
                                    </form>
                                </field>
                                <separator string="Autres Entrées"/>
                                <field name="input_line_ids" colspan="4" nolabel="1" force_save="1">
                                    <tree string="Input Data" editable="bottom">
                                       <field name="name" force_save="1"/>
                                        <field name="code" force_save="1"/>
                                        <field name="amount"/>
                                        <field name="contract_id"/>
                                        <field name="sequence" invisible="True"/>
                                    </tree>
                                    <form string="Payslip Line">
                                        <group col="4">
                                            <field name="name"/>
                                            <field name="code"/>
                                            <field name="sequence"/>
                                            <field name="amount"/>
                                            <field name="contract_id"/>
                                        </group>
                                    </form>
                                </field>
                            </page>
                </xpath> 
              </data>
           
        </field>
     </record>
</odoo>