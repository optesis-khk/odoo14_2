<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data>

     <record id="travel_request_branch_manager_rule" model="ir.rule">
        <field name="name">Travel Request Access Manager</field>
        <field name="model_id" ref="model_employee_travel_request"/>
        <field name="domain_force">[(1,'=',1)]</field>
        <field name="groups" eval="[(4, ref('branch.group_branch_user_manager'))]"/>
    </record>

    <record id="travel_request_branch_user_rule" model="ir.rule">
        <field name="name">Travel Request Access User</field>
        <field name="model_id" ref="model_employee_travel_request"/>
        <field name="groups" eval="[(4, ref('branch.group_branch_user'))]"/>
        <field name="domain_force">['|',('branch_id','=',False),('branch_id','=',user.branch_id.id)]</field>
    </record>
    

</data>
</odoo>